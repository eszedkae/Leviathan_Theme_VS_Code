{
	"name": "leviathan-theme",
	"type": "dark",
	"colors": {
		// Editor - frosted glass effect with #212121 background
		"editor.background": "#212121",
		"editor.foreground": "#b0b0b0",
		"editorCursor.foreground": "#c3ff45",
		"editor.lineHighlightBackground": "#2a2a2a",
		"editorLineNumber.foreground": "#666666",
		"editorLineNumber.activeForeground": "#999999",
		"editor.selectionBackground": "#c3ff4533",
		"editor.selectionForeground": "#000000",
		"editor.wordHighlightBackground": "#c3ff4522",
		"editor.wordHighlightStrongBackground": "#c3ff4544",
		// Editor Widget (autocomplete, hover, etc.) - frosted effect
		"editorWidget.background": "#212121dd",
		"editorWidget.border": "#333333",
		"editorHoverWidget.background": "#212121dd",
		"editorSuggestWidget.background": "#212121dd",
		// Activity Bar - keep black
		"activityBar.background": "#0a0a0a",
		"activityBar.foreground": "#b0b0b0",
		"activityBar.activeBorder": "#c3ff45",
		"activityBar.activeBackground": "#c3ff4511",
		"activityBarBadge.background": "#c3ff45",
		"activityBarBadge.foreground": "#000000",
		// Side Bar - keep black
		"sideBar.background": "#0a0a0a",
		"sideBar.foreground": "#b0b0b0",
		"sideBarTitle.foreground": "#b0b0b0",
		"sideBarSectionHeader.background": "#0a0a0a",
		"sideBarSectionHeader.foreground": "#b0b0b0",
		"tree.indentGuidesStroke": "#333333",
		// File explorer - bright green selection
		"list.activeSelectionBackground": "#c3ff45",
		"list.activeSelectionForeground": "#000000",
		"list.inactiveSelectionBackground": "#c3ff45",
		"list.inactiveSelectionForeground": "#000000",
		"list.hoverBackground": "#c3ff4522",
		"list.focusBackground": "#c3ff45",
		"list.focusForeground": "#000000",
		// Title Bar - keep black
		"titleBar.activeBackground": "#0a0a0a",
		"titleBar.activeForeground": "#b0b0b0",
		"titleBar.inactiveBackground": "#0a0a0a",
		"titleBar.inactiveForeground": "#666666",
		// Tabs - frosted effect
		"tab.activeBackground": "#212121",
		"tab.activeForeground": "#b0b0b0",
		"tab.activeBorder": "#c3ff45",
		"tab.inactiveBackground": "#0a0a0a",
		"tab.inactiveForeground": "#666666",
		"tab.border": "#333333",
		"editorGroupHeader.tabsBackground": "#0a0a0a",
		// Editor Groups - frosted separator
		"editorGroup.border": "#333333",
		"editorGroupHeader.border": "#333333",
		// Status Bar
		"statusBar.background": "#c3ff45",
		"statusBar.foreground": "#000000",
		// Panel - frosted effect
		"panel.background": "#212121dd",
		"panel.border": "#333333",
		"panelTitle.activeBorder": "#c3ff45",
		"panelTitle.activeForeground": "#b0b0b0",
		// Terminal - frosted effect
		"terminal.background": "#212121",
		"terminal.foreground": "#b0b0b0",
		"terminal.ansiGreen": "#c3ff45",
		"terminal.ansiBrightGreen": "#c3ff45",
		// Scrollbars
		"scrollbarSlider.background": "#c3ff4533",
		"scrollbarSlider.hoverBackground": "#c3ff4555",
		"scrollbarSlider.activeBackground": "#c3ff4577",
		// Input
		"input.background": "#2a2a2a",
		"input.border": "#333333",
		"input.foreground": "#b0b0b0",
		// Button
		"button.background": "#c3ff45",
		"button.foreground": "#000000",
		"button.hoverBackground": "#c3ff45",
		// Badge
		"badge.background": "#c3ff45",
		"badge.foreground": "#000000",
		// Breadcrumb
		"breadcrumb.background": "#212121",
		"breadcrumb.foreground": "#b0b0b0",
		// Minimap
		"minimap.background": "#212121cc"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#666666"
			}
		},
		{
			"name": "Import Keywords",
			"scope": [
				"keyword.control.import",
				"keyword.other.import",
				"keyword.control.from",
				"keyword.control.export"
			],
			"settings": {
				"foreground": "#b96d8a",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Keywords and Storage",
			"scope": [
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"keyword",
				"keyword.operator.new",
				"storage.modifier.private",
				"storage.modifier.public",
				"keyword.control.flow",
				"keyword.control.conditional"
			],
			"settings": {
				"foreground": "#b96d8a",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Function Definitions",
			"scope": [
				"keyword.other.def",
				"storage.type.function"
			],
			"settings": {
				"foreground": "#b96d8a",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Variables and Identifiers",
			"scope": [
				"variable",
				"variable.parameter",
				"variable.other",
				"meta.definition.variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Function Names",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call",
				"entity.name.function.method"
			],
			"settings": {
				"foreground": "#a2b271",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Built-in Functions",
			"scope": [
				"support.function.builtin"
			],
			"settings": {
				"foreground": "#a2b271"
			}
		},
		{
			"name": "Types and Classes",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"storage.type.primitive",
				"entity.name.type.struct",
				"entity.name.type.class"
			],
			"settings": {
				"foreground": "#b0b0b0",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#9c6d4c"
			}
		},
		{
			"name": "Numbers and Integers",
			"scope": [
				"constant.numeric",
				"constant.numeric.integer",
				"constant.numeric.float"
			],
			"settings": {
				"foreground": "#7a7249"
			}
		},
		{
			"name": "Constants and Language Constants",
			"scope": [
				"constant.language",
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#b96d8a"
			}
		},
		{
			"name": "Brackets and Parentheses - WHITE",
			"scope": [
				"punctuation.bracket",
				"punctuation.brace",
				"punctuation.parenthesis",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Other Punctuation",
			"scope": [
				"punctuation",
				"punctuation.definition",
				"punctuation.separator",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.arithmetic",
				"keyword.operator.comparison",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Properties and Attributes",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#b0b0b0"
			}
		},
		{
			"name": "Method Calls",
			"scope": [
				"variable.function",
				"entity.name.function.call"
			],
			"settings": {
				"foreground": "#a2b271"
			}
		}
	],
	"semanticHighlighting": true
}